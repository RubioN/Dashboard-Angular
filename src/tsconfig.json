{
  "compilerOptions": {
    "baseUrl": "./",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "jszip": [
        "../node_modules/jszip/dist/jszip.min.js"
      ]
    },
    "mapRoot": "./",
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es2015",
    "allowSyntheticDefaultImports": true,
    "allowJs":true,
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": [
      "ES2018",
      "dom"
    ]
  }
}
